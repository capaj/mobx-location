{
  "name": "mobx-location",
  "version": "0.1.1",
  "description": "minimal observable wrapper around browser history utilizing popstate event of HTML5 history api",
  "main": "mobx-location.js",
  "scripts": {
    "build": "babel ./src/mobx-location.js --out-file mobx-location.js",
    "pretest": "standard",
    "lintfix": "standard --fix",
    "test": "ava",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capaj/mobx-location.git"
  },
  "keywords": [
    "html5",
    "location",
    "history"
  ],
  "author": "capajj@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capaj/mobx-location/issues"
  },
  "homepage": "https://github.com/capaj/mobx-location#readme",
  "peerDependencies": {
    "mobx": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-env": "^2.0.19",
    "mobx": "^3.0.0",
    "standard": "^8.6.0"
  },
  "standard": {
    "ignore": [
      "mobx-location.js"
    ]
  },
  "ava": {
    "files": [
      "*.spec.js"
    ],
    "require": [
      "babel-register",
      "./ava-init.js"
    ],
    "babel": "inherit"
  }
}
